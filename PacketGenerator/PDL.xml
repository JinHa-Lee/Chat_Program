<?xml version="1.0" encoding="utf-8" ?>
<!-- PDL(Packet Definition List)-->
<PDL>
	<packet name="PlayerInfo">
		<long name ="playerId"/>
		<string name ="playerName"/>
		<list name="SkillInfo">
			<int name ="id"/>
			<short name ="level"/>
			<float name ="duration"/>
		</list>
	</packet>
	<packet name="PlayerChat">
		<long name ="playerId"/>
		<string name ="playerName"/>
		<list name="ChatInfo">
			<int name ="ChatId"/>
		</list>
		<string name ="contents"/>
		<byte name ="ChatByte"/>
	</packet>
</PDL>